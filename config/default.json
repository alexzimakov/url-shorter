{
  "env": "$NODE_ENV",
  "port": "$PORT",
  "database": {
    "engine": "mongo",
    "url": "$DB_URL",
    "options": {}
  },
  "salt": "$SALT",
  "secret": "$SECRET",
  "jwt": {
    "expiresIn": "32 days",
    "subject": "auth"
  },
  "pagination": {
    "skip": 0,
    "limit": 20
  },
  "crypto": {
    "iterations": 100000,
    "keylen": 128,
    "algorithm": "sha512"
  }
}